st params = pageSize + pageNum\n\nVue.component('cards', {\n  data: function () {\n    return {\n      cards: null,\n      loading: true,\n      errored: false\n    }\n  },\n  mounted () {\n    axios\n      .get(url + params)\n      .then(response => {\n        this.cards = response.data.cards\n        console.log('getCards: ' + this.cards)\n      })\n      .catch(error => {\n        console.log(error)\n        this.errored = true\n      })\n      .finally(() => {\n        this.loading = false\n      })\n  }\n})\n\nexport default {\n  name: 'CardsMain',\n  props: [\n    'cards'\n  ]\n}\n</script>\n\n<template>\n  <div>\n    <h1>H1</h1>\n    <p>\n      Pull the data on the cards and display it here.\n    </p>\n    <div id=\"cards\">\n    <section v-if=\"errored\">\n      <p>We're sorry, we're not able to retrieve this information at the moment, please try back later</p>\n    </section>\n      <section v-else>\n        <div v-if=\"loading\">Loading...</div>\n          <div>\n              {{cardsList}}\n          </div>\n          <div v-for=\"card in cardsList\" v-bind:key=\"card.id\">\n            <span class=\"card\">\n              <span :id=\"card.id\">>{{ card.name }}</span>\n            </span>\n          </div>    \n      </section>\n    </div>\n  </div>\n</template>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped lang=\"scss\">\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}a\na {\n  color: #42b983;\n}\n</style>"]}]}